{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@now/node"
    },
    {
      "src": "post/post-index.js",
      "use": "@now/node"
    },
    {
      "src": "api/swagger.json",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/user(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/post(.*)",
      "dest": "/post/post-index.js"
    },
    {
      "src": "/api-docs(.*)",
      "dest": "api/index.js"
    }
  ]
}
